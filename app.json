{
  "expo": {
    "name": "리멤버미",
    "slug": "rememberme-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/image/nutty-no-bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#f5efcd"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.coco.rememberme",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/image/coco-icon.png",
        "backgroundColor": "#f5efcd"
      },
      "googleServicesFile": "./.eas/.env/GOOGLE_SERVICES_JSON",
      "package": "com.coco.rememberme",
      "permissions": ["READ_MEDIA_IMAGES", "CAMERA"]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/image/coco-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3a2b2e44-3fa0-4c92-892c-ba96f4869589"
      }
    },
    "owner": "ms-coco",
    "plugins": ["expo-secure-store"]
  }
}
